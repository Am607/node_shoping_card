 <section>
        <div class="container mt-5">
            <div class="row">
                {{#each products}}
                <div class="col-md-3">
                    <div class="card" style="width: 18rem;">
                        <img src="/product-images/{{this.products._id}}.jpg"
                            class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">{{this.products.Name}}</h5>
                             <h5 class="card-title">{{this.products.Price }}</h5>
                              <h5 class="card-title">{{this.products.Category}}</h5>
                            <p class="card-text">{{this.products.Description}}</p>
                          
                           
                        </div>
                    </div>
                </div>
                {{/each}}
             
            </div>
        </div>
    </section>